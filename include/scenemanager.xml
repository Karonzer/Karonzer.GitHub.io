		<hotspot name="gh_gsb_popup_f" style="jd_gdm_popup | scale_4" ath="0" atv="-8"  rx="-8" ry="0" rz="0" zorder="16" />
		<hotspot name="gh_gsb_popup_b" style="jd_gdm_popup | scale_4" ath="180" atv="-8"  rx="-8" ry="0" rz="0" zorder="16" />



	</scene>
