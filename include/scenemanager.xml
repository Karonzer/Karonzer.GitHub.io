<krpano>

	<style name="go_gh_gsb" url="%FIRSTXML%/source/common/panos/commonmap/2048/strt_btn.png" zorder="1" zoom="false"  distorted="true" handcursor="false" />
	<style name="go_gh_gsb_over" url="%FIRSTXML%/source/common/panos/commonmap/2048/strt_btn_click.png" zorder="1" zoom="false" visible="false" distorted="true" handcursor="false" />

	<style name="gh_gsb_popup" url="%FIRSTXML%/source/common/panos/commonmap/2048/vr_popup.png" zorder="1" zoom="false"  distorted="true" handcursor="false" />

	<!-- 지역 지도 노드 -->
	<scene name="commonmap" title="VR 역사 답사"  lat="" lng="" heading="" onstart="delayedcall(0.1,BGM(gh_gsb););">
		<preview url="%FIRSTXML%/source/common/panos/commonmap/commonmap_preview.jpg" />
		<view hlookat="0" vlookat="0"  fovtype="MFOV" fov="120" fovmin="110" fovmax="130" limitview="off" hlookatmin="-0" hlookatmax="0" vlookatmin="0" vlookatmax="0" />
		<control usercontrol="off"/>
		<image>
			<cube url="%FIRSTXML%/source/common/panos/commonmap/2048/vr_air2_%s.png" />
		</image>

		<hotspot name="go_gh_gsb_f" style="go_gh_gsb | scale_6" atv="29"  rx="30" ry="0" rz="0" onclick="start_tour(gh_gsb);  set(control.usercontrol, all);"  onhover="btn_hover(go_gh_gsb_f, go_gh_gsb_over_f);" zorder="16" />
		<hotspot name="go_gh_gsb_over_f" style="go_gh_gsb_over | scale_6"  atv="29" rx="30" ry="0" rz="0" onclick="start_tour(gh_gsb); set(control.usercontrol, all);" onout="btn_out(go_gh_gsb_f, go_gh_gsb_over_f);" zorder="16" />

		<hotspot name="go_gh_gsb_b" style="go_gh_gsb | scale_6" ath="180" atv="29" rx="30" ry="0" rz="0" onclick="start_tour(gh_gsb); set(control.usercontrol, all);" onhover="btn_hover(go_gh_gsb_b, go_gh_gsb_over_b);" zorder="16"/>
		<hotspot name="go_gh_gsb_over_b" style="go_gh_gsb_over | scale_6" ath="180" atv="29" rx="30" ry="0" rz="0" onclick="start_tour(gh_gsb); set(control.usercontrol, all);" onout="btn_out(go_gh_gsb_b, go_gh_gsb_over_b);" zorder="16" />

		<hotspot name="gh_gsb_popup_f" style="gh_gsb_popup | scale_4" ath="0" atv="-8"  rx="-8" ry="0" rz="0" zorder="16" />
		<hotspot name="gh_gsb_popup_b" style="gh_gsb_popup | scale_4" ath="180" atv="-8"  rx="-8" ry="0" rz="0" zorder="16" />
		
	</scene>
<include url="%FIRSTXML%/include/Scene/gh_gsb.xml" />

</krpano>
