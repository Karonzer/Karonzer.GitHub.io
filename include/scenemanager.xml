		<image>
			<cube url="%FIRSTXML%/source/common/panos/commonmap/2048/vr_air2_%s.png" />
		</image>

		<hotspot name="go_jd_gdm_f" style="go_jd_gdm | scale_6" atv="29"  rx="30" ry="0" rz="0" onclick="start_tour(jd_gdm);  set(control.usercontrol, all);"  onhover="btn_hover(go_jd_gdm_f, go_jd_gdm_over_f);" zorder="16" />
		<hotspot name="go_jd_gdm_over_f" style="go_jd_gdm_over | scale_6"  atv="29" rx="30" ry="0" rz="0" onclick="start_tour(jd_gdm); set(control.usercontrol, all);" onout="btn_out(go_jd_gdm_f, go_jd_gdm_over_f);" zorder="16" />

